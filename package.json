{
  "name": "react-scheduler",
  "version": "1.0.0",
  "description": "React-scheduler",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "start": "cross-env NODE_ENV=development FAKE=true STAND_ALONE=true webpack-dev-server",
    "build": "npm test && tslint src/**/*.ts{,x} && cross-env NODE_ENV=production webpack",
    "postinstall": "typings install"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/ts-jest/preprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs",
        "jsx": "react"
      }
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "3.8.1",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^2.2.4",
    "cross-env": "^3.1.1",
    "css-loader": "0.23.1",
    "ejs-loader": "0.3.0",
    "enzyme": "2.4.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "^2.0.0",
    "jest": "15.1.1",
    "json-loader": "^0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "path": "0.12.7",
    "react-addons-perf": "^15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-hot-loader": "^1.3.0",
    "react-test-renderer": "15.3.1",
    "style-loader": "0.13.1",
    "ts-jest": "0.1.5",
    "tslint": "^3.15.1",
    "typescript": "2.0.3",
    "typings": "^1.4.0",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.16.1",
    "webpack-merge": "^0.14.1"
  }
}
